{
    "name": "javascripts",
    "version": "1.0.1",
    "scripts": {
        "build": "gulp prod:build && gulp unit:karma",
        "tdd": "gulp tdd:karma && gulp tdd:watch",
        "karma-stop": "karma stop --port=9876"
    },
    "keywords": [],
    "author": "Steve Fazekas",
    "devDependencies": {
        "gulp-concat": "^2.6.1",
        "gulp-debug": "^3.1.0",
        "gulp-eslint": "^3.0.1",
        "gulp-filelog": "^0.4.1",
        "gulp-if": "^2.0.2",
        "gulp-istanbul": "^1.1.1",
        "gulp-jscpd": "0.0.7",
        "gulp-load-plugins": "^1.5.0",
        "gulp-open": "^2.0.0",
        "gulp-plumber": "^1.1.0",
        "gulp-run": "^1.7.1",
        "gulp-sourcemaps": "^2.4.1",
        "gulp-util": "^3.0.8",
        "gulp-watch": "^4.3.11",
        "imports-loader": "^0.7.1",
        "jasmine-ajax": "^3.3.1",
        "jasmine-core": "^2.5.2",
        "jasmine-fixture": "^2.0.0",
        "junit-viewer": "^4.11.0",
        "karma": "^1.5.0",
        "karma-chrome-launcher": "^2.0.0",
        "karma-coverage": "^1.1.1",
        "karma-firefox-launcher": "^1.0.1",
        "karma-html-reporter": "^0.2.7",
        "karma-ie-launcher": "^1.0.0",
        "karma-jasmine": "^1.1.0",
        "karma-jasmine-jquery-2": "^0.1.1",
        "karma-jasmine-spec-runner-reporter": "^0.1.5",
        "karma-jquery": "^0.2.0",
        "karma-junit-reporter": "^1.2.0",
        "karma-phantomjs-launcher": "^1.0.2",
        "karma-spec-reporter": "0.0.30",
        "phantomjs-polyfill": "0.0.2",
        "require-dir": "^0.3.1",
        "run-sequence": "^1.2.2",
        "yargs": "^7.0.1"
    },
    "description": "NA",
    "dependencies": {
        "gulp": "^3.9.1"
    },
    "eslintConfig": {
        "env": {
            "node": true,
            "builtin": true,
            "es6": true
        },
        "globals": {
            "$": true,
            "APP": true,
            "document": true,
            "window": true,
            "jQuery": true
        },
        "rules": {
            "block-scoped-var": 0,
            "camelcase": 0,
            "comma-spacing": [
                1,
                {
                    "before": false,
                    "after": true
                }
            ],
            "consistent-return": 0,
            "curly": [
                2,
                "all"
            ],
            "dot-notation": [
                0,
                {
                    "allowKeywords": true
                }
            ],
            "eqeqeq": [
                0,
                "allow-null"
            ],
            "global-strict": [
                0,
                "never"
            ],
            "guard-for-in": 0,
            "indent": [
                1,
                2,
                {
                    "SwitchCase": 1,
                    "VariableDeclarator": 1
                }
            ],
            "lines-around-comment": 0,
            "key-spacing": 0,
            "keyword-spacing": 1,
            "new-cap": 0,
            "no-alert": 0,
            "no-bitwise": 2,
            "no-caller": 2,
            "no-cond-assign": [
                2,
                "except-parens"
            ],
            "no-debugger": 2,
            "no-dupe-args": 2,
            "no-dupe-keys": 2,
            "no-empty": 2,
            "no-eval": 0,
            "no-extend-native": 2,
            "no-extra-bind": 2,
            "no-extra-parens": 0,
            "no-extra-semi": 2,
            "no-func-assign": 2,
            "no-implied-eval": 2,
            "no-invalid-regexp": 2,
            "no-irregular-whitespace": 1,
            "no-iterator": 2,
            "no-loop-func": 2,
            "no-mixed-requires": 0,
            "no-multi-str": 2,
            "no-multi-spaces": 1,
            "no-native-reassign": 2,
            "no-new": 2,
            "no-param-reassign": 0,
            "no-proto": 2,
            "no-redeclare": 0,
            "no-script-url": 2,
            "no-self-assign": 2,
            "no-self-compare": 2,
            "no-sequences": 2,
            "no-shadow": 0,
            "no-undef": 0,
            "no-underscore-dangle": 0,
            "no-unreachable": 1,
            "no-unused-vars": 0,
            "no-use-before-define": 0,
            "no-useless-call": 2,
            "no-useless-concat": 0,
            "no-with": 2,
            "quotes": [
                0,
                "single"
            ],
            "radix": 2,
            "semi": [
                0,
                "never"
            ],
            "strict": 0,
            "space-before-blocks": 1,
            "space-before-function-paren": [
                1,
                {
                    "anonymous": "always",
                    "named": "never"
                }
            ],
            "space-in-parens": [
                1,
                "never"
            ],
            "space-infix-ops": 1,
            "valid-typeof": 2,
            "vars-on-top": 0,
            "wrap-iife": [
                2,
                "inside"
            ]
        }
    }
}
